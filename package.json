{
  "name": "abroadwith-frontend",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "aws-sdk": "^2.2.39",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "^1.15.0",
    "braintree": "^1.36.0",
    "browserify": "^12.0.1",
    "browserify-shim": "^3.8.12",
    "clean-css": "^3.4.10",
    "cookie-parser": "^1.4.1",
    "disc": "^1.3.2",
    "dropzone": "^4.3.0",
    "exif-parser": "^0.1.9",
    "express": "^4.13.3",
    "google-maps": "^3.2.1",
    "i18next": "^2.0.20",
    "i18next-xhr-backend": "^0.3.0",
    "jquery": "^2.2.0",
    "js-cookie": "^2.1.0",
    "jsonwebtoken": "^5.5.4",
    "jwt-decode": "^1.5.1",
    "lwip": "0.0.8",
    "markerwithlabel": "^1.1.13",
    "materialize-css": "git+https://github.com/SourceComb/materialize.git",
    "multer": "^1.1.0",
    "node-gyp": "^3.3.0",
    "node-sass": "^3.4.2",
    "nunjucks": "^2.2.0",
    "pikaday": "^1.4.0",
    "rating": "^0.1.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-lite": "0.0.21",
    "react-nouislider": "^1.9.1",
    "select2-browserify": "^3.4.5",
    "uglifyify": "^3.0.1",
    "uglifyjs": "^2.4.10",
    "uncss": "^0.13.0",
    "wallop": "^2.2.3",
    "winston": "^2.2.0",
    "winston-daily-rotate-file": "^1.0.1",
    "wnumb": "^1.0.2"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "application-settings": "./app.settings.json",
    "jquery": "./node_modules/jquery/dist/jquery.min.js",
    "materialize": "./src/materialize.js",
    "react": "react-lite",
    "react-dom": "react-lite",
    "react-nouislider": "./node_modules/react-nouislider/dist/react-nouislider.common",
    "domains": "./src/global/constants/domains.json",
    "i18n": "./src/global/util/i18n",
    "languages": "./src/global/constants/UserLanguages.json",
    "languages-module": "./src/global/components/LanguageModule/languages-module.react",
    "login": "./src/global/login",
    "login-redirect": "./src/global/login-redirect",
    "logged-in": "./src/global/logged-in",
    "logout": "./src/global/logout",
    "refresh-token": "./src/global/refresh-token",
    "no-ui-slider": "./src/nouislider.min",
    "process-language-chips": "./src/global/process-language-chips",
    "randomise-coordinate": "./src/global/randomise-coordinate",
    "send-payment-nonce": "./src/global/send-payment-nonce",
    "toast": "./src/global/toast",
    "currencies": "./src/global/constants/Currencies.json",
    "default-bank-currencies": "./src/global/constants/DefaultBankCurrencies",
    "default-paypal-currencies": "./src/global/constants/DefaultPaypalCurrencies",
    "JWT": "./src/global/jwt",
    "GET": "./src/global/ajax/GET",
    "POST": "./src/global/ajax/POST",
    "DELETE": "./src/global/ajax/DELETE",
    "new-message-thread": "./src/global/new-message-thread",
    "api-date": "./src/utils/api-date",
    "ui-date": "./src/utils/ui-date",
    "validate-book-now-buttons": "./src/global/validate-book-now-buttons",
    "validate-message-buttons": "./src/global/validate-message-buttons",
    "verifications-module-init": "./src/global/components/VerificationsModule/verifications-module",
    "is-on-screen": "./src/utils/is-on-screen",
    "home-status-codes": "./src/global/constants/HomeStatusCodes.json"
  },
  "browserify-shim": {
    "jquery": "$",
    "materialize": "materialize",
    "react-nouislider": {
      "depends": "react"
    }
  },
  "scripts": {
    "directories": "mkdir build && cd build && mkdir backend && mkdir locales",
    "neuralyzer": "rm -rf node_modules build",
    "clean": "rm -rf build",
    "export": "cp -R locales/* build/locales && cp -R sitemap/* build/",
    "sass": "node-sass --output-style compressed -o build/css/ scss/",
    "sass-watch": "node-sass --output-style compressed -w -o build/css/ scss/",
    "bundle": "browserify -t [ babelify --presets [ react ] ]  src/app.js > build/app.js -i moment",
    "bundle-min": "browserify -t [ babelify --presets [ react ] ] -g [ uglifyify ] src/app.js > build/app.js -i moment",
    "uncss": "uncss http://localhost:3000 > build/css/app.css",
    "test": "put test file here",
    "discify": "browserify -t [ babelify --presets [ react ] ] -g [ uglifyify ] src/app.js > build/app.js -i moment --full-paths && discify build/app.js > build/disc.html --open",
    "build": "npm run clean && npm run directories && npm run export && npm run sass && npm run bundle",
    "start": "node src/server.js",
    "fullstart": "npm run build && npm run start",
    "browsersync": "browser-sync start --reload-delay 1000 --proxy 'http://localhost:3000' --files 'build/css/*.css, build/app.js, src/*/*.html, src/*/*/*.html'",
    "production": "npm run build && npm run bundle-min"
  },
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/abroadwith/frontend.git"
  },
  "homepage": "https://bitbucket.org/abroadwith/frontend#readme"
}
